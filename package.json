{
  "name": "call-for-cofounder-investor",
  "version": "1.0.0",
  "description": "Call for Investor/CoFounder Mobile Web App",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"blue,green\" \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "start": "concurrently -n \"BACKEND,FRONTEND\" -c \"blue,green\" \"npm start --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=frontend",
    "install:all": "npm install",
    "kill-port": "node scripts/kill-port.js",
    "kill-port:3000": "node scripts/kill-port.js 3000",
    "kill-port:5173": "node scripts/kill-port.js 5173"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
